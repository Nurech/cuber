{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cuber": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/cuber",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": [
              "assets/js/cuber/src/scripts/vendor/threejs/src/Three.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/core/EventDispatcher.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/math/Math.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/math/Quaternion.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/math/Vector2.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/math/Vector3.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/math/Box3.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/math/Sphere.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/math/Euler.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/math/Matrix3.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/math/Matrix4.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/core/Object3D.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/cameras/Camera.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/cameras/PerspectiveCamera.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/math/Ray.js",
              "assets/js/cuber/src/scripts/vendor/threejs/src/math/Plane.js",
              "assets/js/cuber/src/scripts/vendor/tween.min.js",
              "assets/js/cuber/src/scripts/ERNO.js",
              "assets/js/cuber/src/scripts/vendor/CSS3DRenderer.js",
              "assets/js/cuber/src/scripts/utils/utils.js",
              "assets/js/cuber/src/scripts/utils/Number.js",
              "assets/js/cuber/src/scripts/utils/String.js",
              "assets/js/cuber/src/scripts/utils/Array.js",
              "assets/js/cuber/src/scripts/colors.js",
              "assets/js/cuber/src/scripts/directions.js",
              "assets/js/cuber/src/scripts/queues.js",
              "assets/js/cuber/src/scripts/twists.js",
              "assets/js/cuber/src/scripts/cubelets.js",
              "assets/js/cuber/src/scripts/groups.js",
              "assets/js/cuber/src/scripts/slices.js",
              "assets/js/cuber/src/scripts/folds.js",
              "assets/js/cuber/src/scripts/projector.js",
              "assets/js/cuber/src/scripts/interaction.js",
              "assets/js/cuber/src/scripts/controls.js",
              "assets/js/cuber/src/scripts/cubes.js",
              "assets/js/cuber/src/scripts/solvers.js",
              "assets/js/cuber/src/scripts/renderer.js",
              "assets/js/cuber/examples/basic/scripts/jquery.js",
              "assets/js/cuber/src/scripts/extras/renderers/iecss3d.js",
              "assets/js/cuber/src/scripts/extras/renderers/ierenderer.js",
              "assets/js/cuber/src/scripts/extras/controls/locked.js",
              "assets/js/cuber/src/scripts/extras/deviceMotion.js",
              "assets/js/cuber/examples/basic/scripts/presets.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cuber:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cuber:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cuber:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "cuber:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cuber:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "cuber"
}
